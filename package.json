{
  "name": "@tailwind-rc/date-picker",
  "version": "0.0.4",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:Apozhidaev/tailwindcss-date-picker.git",
  "author": "alexanderpo <alexander.pozhidaev@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "range",
    "src"
  ],
  "scripts": {
    "build:ts": "tsc",
    "build:cjs": "esbuild src/index.tsx --format=cjs --bundle --outfile=dist/index.js --external:@heroicons/* --external:react* --external:classnames",
    "build:esm": "esbuild src/index.tsx --format=esm --bundle --outfile=dist/module.js --external:@heroicons/* --external:react* --external:classnames",
    "build:range:cjs": "esbuild src/range.tsx --format=cjs --bundle --outfile=dist/range.js --external:@heroicons/* --external:react* --external:classnames",
    "build:range:esm": "esbuild src/range.tsx --format=esm --bundle --outfile=dist/range.module.js --external:@heroicons/* --external:react* --external:classnames",
    "build": "yarn run build:cjs && yarn run build:esm && yarn run build:range:cjs && yarn run build:range:esm && yarn run build:ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "classnames": "^2.3.2",
    "react-easepick": "^0.0.6",
    "react-use-event-hook": "^0.9.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "esbuild": "^0.16.9",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  }
}
